<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nju.edu.erp.dao.SaleDetailDAO">
    <select id="getSailDetail" resultMap="SaleDetailPO">
        select *
        from sale_detail;
    </select>
    <resultMap id="SaleDetailPO" type="com.nju.edu.erp.model.po.SaleDetailPO">
        <result column="create_time" property="createTime"/>
        <result column="pid" property="pid"/>
        <result column="SellerId" property="sellerId"/>
        <result column="unit_price" property="unit_price"/>
        <result column="total_price" property="total_price"/>
        <result column="quantity" property="quantity"/>
    </resultMap>

</mapper>
