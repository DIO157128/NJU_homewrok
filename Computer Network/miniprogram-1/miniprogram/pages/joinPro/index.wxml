<!--pages/joinPro/index.wxml-->
<view class="container">
  <view class="form-group">
    <view class="form-cell">
      <view class="form-cell_title">项目邀请码</view>
      <input class="form-cell_input" placeholder="请输入项目邀请码" placeholder-class="form-cell_title-placeholder"
        bindinput="onGroupInput" value="{{groupId}}" />
    </view>
  </view>
  <view class="footer">
    <view class="reset" bindtap="reset">重置</view>
    <view class="save" bindtap="joinGroup">加入项目</view>
  </view>
</view>